// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Movimentacao {
  id String @id @default(uuid())
  nome String
  descricao String
  tipo String
  valor Float
  data DateTime @updatedAt
  @@map("movimentacoes")
}